livingroom_ac_20c:
  alias: Livingroom AC 20c Cool
  sequence:
  - service: switch.turn_on
    entity_id: switch.mazgan_salon_20c
livingroom_ac_off:
  alias: Livingroom AC Off
  sequence:
  - condition: template
    value_template: '{{ is_state(''binary_sensor.lumi_lumi_sensor_magnet_0357e3e8_1_6'',''on'') }}'
  - service: switch.turn_off
    entity_id: switch.mazgan_salon_off
val_office_ac_20c:
  alias: Val AC 20c Cool
  sequence:
  - service: switch.turn_on
    entity_id: switch.val_office_ac_20c
office_ac_off:
  alias: Val AC Off
  sequence:
  - condition: template
    value_template: '{{ is_state(''binary_sensor.lumi_lumi_sensor_magnet_03f880b7_1_6'',''on'') }}'
  - service: switch.turn_off
    entity_id: switch.val_office_ac_off
badroom_ac_30min:
  alias: AC 30 Min
  sequence:
  - service: switch.turn_on
    entity_id: switch.bedroom_ac
  - delay:
      minutes: 30
  - service: switch.turn_off
    entity_id: switch.bedroom_ac
  - delay:
      seconds: 5
badroom_ac_1hour:
  alias: AC 1 Hour
  sequence:
  - service: switch.turn_on
    entity_id: switch.bedroom_ac
  - delay:
      hours: 1
  - service: switch.turn_off
    entity_id: switch.bedroom_ac
  - delay:
      seconds: 5
badroom_ac_2hour:
  alias: AC 2 Hours
  sequence:
  - service: switch.turn_on
    entity_id: switch.bedroom_ac
  - delay:
      hours: 2
  - service: switch.turn_off
    entity_id: switch.bedroom_ac
  - delay:
      seconds: 5
living_room_ac_check_state_script:
  sequence:
  - service_template: >
      {% if (selected_mode.lower() == "cool") %}
         {% if (selected_fan.lower() == "low") %} script.tornado_ac_cool_low_script
         {% elif (selected_fan.lower() == "med") %} script.tornado_ac_cool_med_script
         {% elif (selected_fan.lower() == "high") %} script.tornado_ac_cool_high_script
         {% elif (selected_fan.lower() == "auto") %} script.tornado_ac_cool_auto_script
         {% endif %}
      {%-elif (selected_mode.lower() == "heat") %}
         {% if (selected_fan.lower() == "low") %} script.tornado_ac_heat_low_script
         {% elif (selected_fan.lower() == "med") %} script.tornado_ac_heat_med_script
         {% elif (selected_fan.lower() == "high") %} script.tornado_ac_heat_high_script
         {% elif (selected_fan.lower() == "auto") %} script.tornado_ac_heat_auto_script
         {% endif %}
      {% endif %}
    data_template:
      selected_temp: '{{ selected_temp }}'
      broadlink_ip: !secret lr_bradlink_ip
